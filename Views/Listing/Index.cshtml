@using yelp.Models
@model List<Listing>
<div class="row">
    <div class="col-sm-10">
        <p>All listings</p>
        <ul>
        @foreach (var listing in Model)
        {
            <li><a href="/listings/@listing.ListingId">@listing.Name</a></li>
        }
        </ul> 
    </div>
    <div class="col-sm-2">
        <p>A Map</p>
    </div>
</div>
